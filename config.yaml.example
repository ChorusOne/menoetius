pushgateway: http://prometheus.example.com:9091
log_level: info
log_format: "%(asctime)-15s [%(levelname)s] (%(threadName)-10s) %(message)s"
log_file: menoetius.log
request_timeout: 5
endpoints:
  - name: monitor
    scheme: http
    host: localhost
    port: 9100
    path: /metrics
    interval: 5
